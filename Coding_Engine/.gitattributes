# Git LFS tracking for Java language package
# Track all binary and large files in language-packages/java/

# Shared libraries
language-packages/java/**/*.so filter=lfs diff=lfs merge=lfs -text

# Java archives and modules
language-packages/java/**/*.jar filter=lfs diff=lfs merge=lfs -text
language-packages/java/**/*.jmod filter=lfs diff=lfs merge=lfs -text

# Archive files
language-packages/java/**/*.tar.gz filter=lfs diff=lfs merge=lfs -text
language-packages/java/**/*.zip filter=lfs diff=lfs merge=lfs -text

# Binary executables in bin directory
language-packages/java/**/bin/* filter=lfs diff=lfs merge=lfs -text

# Other binary files
language-packages/java/**/jexec filter=lfs diff=lfs merge=lfs -text
language-packages/java/**/jspawnhelper filter=lfs diff=lfs merge=lfs -text
language-packages/java/**/modules filter=lfs diff=lfs merge=lfs -text

# Binary data files
language-packages/java/**/*.dat filter=lfs diff=lfs merge=lfs -text
language-packages/java/**/*.certs filter=lfs diff=lfs merge=lfs -text

# Additional binary files without standard extensions
language-packages/java/**/ct.sym filter=lfs diff=lfs merge=lfs -text
language-packages/java/**/cacerts filter=lfs diff=lfs merge=lfs -text

# Git LFS tracking for Python language package
# Track all binary and large files in language-packages/python/

# Shared libraries and extensions
language-packages/python/**/*.so filter=lfs diff=lfs merge=lfs -text

# Static libraries
language-packages/python/**/*.a filter=lfs diff=lfs merge=lfs -text

# Python bytecode (compiled Python files)
language-packages/python/**/*.pyc filter=lfs diff=lfs merge=lfs -text
language-packages/python/**/*.pyo filter=lfs diff=lfs merge=lfs -text

# Archive files
language-packages/python/**/*.tar filter=lfs diff=lfs merge=lfs -text
language-packages/python/**/*.tar.gz filter=lfs diff=lfs merge=lfs -text
language-packages/python/**/*.tar.xz filter=lfs diff=lfs merge=lfs -text
language-packages/python/**/*.tar.bz2 filter=lfs diff=lfs merge=lfs -text
language-packages/python/**/*.zip filter=lfs diff=lfs merge=lfs -text
language-packages/python/**/*.whl filter=lfs diff=lfs merge=lfs -text
language-packages/python/**/*.egg filter=lfs diff=lfs merge=lfs -text

# Binary executables in bin directory
language-packages/python/**/bin/python* filter=lfs diff=lfs merge=lfs -text

# Git LFS tracking for GCC language package
# Track all binary and large files in language-packages/gcc/

# Shared libraries
language-packages/gcc/**/*.so filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.so.* filter=lfs diff=lfs merge=lfs -text

# Static libraries
language-packages/gcc/**/*.a filter=lfs diff=lfs merge=lfs -text

# Libtool archives
language-packages/gcc/**/*.la filter=lfs diff=lfs merge=lfs -text

# Message catalogs
language-packages/gcc/**/*.mo filter=lfs diff=lfs merge=lfs -text

# Info documentation files
language-packages/gcc/**/*.info filter=lfs diff=lfs merge=lfs -text

# Archive files
language-packages/gcc/**/*.tar.gz filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.tar filter=lfs diff=lfs merge=lfs -text

# Binary executables in bin directory
language-packages/gcc/**/bin/* filter=lfs diff=lfs merge=lfs -text

# Binary executables in libexec directory
language-packages/gcc/**/libexec/**/* filter=lfs diff=lfs merge=lfs -text

# Large header files (C/C++ headers, D headers, template files)
language-packages/gcc/**/*.h filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.hpp filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.d filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.tcc filter=lfs diff=lfs merge=lfs -text

# Python files
language-packages/gcc/**/*.py filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.pyc filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.pyo filter=lfs diff=lfs merge=lfs -text

# Man pages (can be large)
language-packages/gcc/**/*.1 filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.7 filter=lfs diff=lfs merge=lfs -text

# Binary files without extensions in specific directories
language-packages/gcc/**/bin/gcc* filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/bin/g++* filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/bin/c++* filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/bin/cpp* filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/bin/gcov* filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/bin/gfortran* filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/bin/gdc* filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/bin/lto-dump filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/bin/x86_64-pc-linux-gnu-* filter=lfs diff=lfs merge=lfs -text

# Plugin libraries
language-packages/gcc/**/plugin/**/*.so filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/plugin/**/*.so.* filter=lfs diff=lfs merge=lfs -text

# State files and other binary data
language-packages/gcc/**/*.state filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/gtype.state filter=lfs diff=lfs merge=lfs -text

# Compiled object files
language-packages/gcc/**/*.o filter=lfs diff=lfs merge=lfs -text

# Binary data files
language-packages/gcc/**/*.dat filter=lfs diff=lfs merge=lfs -text

# Header files without extensions (C++ standard library headers)
language-packages/gcc/**/include/**/* filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/include/c++/**/* filter=lfs diff=lfs merge=lfs -text

# Fortran source files
language-packages/gcc/**/*.f90 filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.f filter=lfs diff=lfs merge=lfs -text
language-packages/gcc/**/*.F filter=lfs diff=lfs merge=lfs -text

# Spec files
language-packages/gcc/**/*.spec filter=lfs diff=lfs merge=lfs -text

# Definition files
language-packages/gcc/**/*.def filter=lfs diff=lfs merge=lfs -text

# Git LFS tracking for Node.js language package
# Track all binary and large files in language-packages/node/

# Binary executables in bin directory
language-packages/node/**/bin/* filter=lfs diff=lfs merge=lfs -text

# Archive files
language-packages/node/**/*.tar.gz filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.tar filter=lfs diff=lfs merge=lfs -text

# Shared libraries
language-packages/node/**/*.so filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.so.* filter=lfs diff=lfs merge=lfs -text

# Native Node.js modules
language-packages/node/**/*.node filter=lfs diff=lfs merge=lfs -text

# Large JavaScript files (especially in node_modules)
language-packages/node/**/*.js filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.cjs filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.mjs filter=lfs diff=lfs merge=lfs -text

# Header files (C/C++ headers)
language-packages/node/**/*.h filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.hpp filter=lfs diff=lfs merge=lfs -text

# Python files (node-gyp and other tools)
language-packages/node/**/*.py filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.pyc filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.pyo filter=lfs diff=lfs merge=lfs -text

# Documentation files (can be large)
language-packages/node/**/*.md filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.html filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.1 filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/*.7 filter=lfs diff=lfs merge=lfs -text

# Corepack shims and executables
language-packages/node/**/lib/node_modules/corepack/dist/**/* filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/lib/node_modules/corepack/shims/* filter=lfs diff=lfs merge=lfs -text

# npm binaries and large modules
language-packages/node/**/lib/node_modules/npm/bin/* filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/lib/node_modules/npm/node_modules/node-gyp/**/*.py filter=lfs diff=lfs merge=lfs -text

# Static libraries
language-packages/node/**/*.a filter=lfs diff=lfs merge=lfs -text

# Compiled object files
language-packages/node/**/*.o filter=lfs diff=lfs merge=lfs -text

# Binary data files
language-packages/node/**/*.dat filter=lfs diff=lfs merge=lfs -text

# Large text files (LICENSE, CHANGELOG, etc.)
language-packages/node/**/LICENSE filter=lfs diff=lfs merge=lfs -text
language-packages/node/**/CHANGELOG.md filter=lfs diff=lfs merge=lfs -text

